{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "nvm use && serverless",
    "test": "jest --watch --silent"
  },
  "dependencies": {
    "classnames": "2.3.1",
    "formik": "2.2.6",
    "mongodb": "^3.6.9",
    "next": "10.0.2",
    "next-compose-plugins": "2.2.1",
    "node-fetch": "2.6.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-bootstrap-icons": "1.4.0",
    "react-dark-mode-toggle": "^0.2.0",
    "react-dom": "17.0.2",
    "react-google-autocomplete": "^2.4.0",
    "react-html-parser": "^2.0.2",
    "react-image-file-resizer": "0.4.2",
    "react-image-gallery": "1.0.9",
    "react-lifecycles-compat": "3.0.4",
    "react-star-ratings": "2.3.0",
    "react-switch": "6.0.0",
    "react-timeago": "^6.2.1",
    "react-tooltip": "^4.2.19",
    "styled-components": "5.2.3",
    "typed.js": "2.0.12"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "babel-jest": "^27.0.6",
    "babel-plugin-styled-components": "1.12.0",
    "jest": "^27.0.6",
    "jest-canvas-mock": "^2.3.1",
    "next-page-tester": "^0.27.0"
  },
  "jest": {"testEnvironment": "jsdom",
  "setupFilesAfterEnv": ["./__tests__/config/jest.setup.js"],
  "setupFiles": ["jest-canvas-mock"],
  "collectCoverageFrom": [
    "**/*.{js,jsx,ts,tsx}",
    "!**/*.d.ts",
    "!**/node_modules/**"
  ],
  "testPathIgnorePatterns": ["/node_modules/", "/.next/", "/__tests__/config/"],
  "transformIgnorePatterns": [
    "/node_modules/",
    "^.+\\.module\\.(css|sass|scss)$"
  ],
  "moduleNameMapper": {
    "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
  }}
}
